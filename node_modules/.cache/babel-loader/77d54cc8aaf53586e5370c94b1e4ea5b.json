,GAAA,IAAA;AACA,OALA,EAMA,OANA,CAMA;AAAA,eAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AAAA,OANA;AAOA,KAZA;AAaA,IAAA,OAbA,qBAaA;AAAA;;AACA,MAAA,IAAA,CACA,IADA,CACA,oBAAA,KAAA,GADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,KAAA,IAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,aAAA,CAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA,SAHA,MAGA;AACA,UAAA,KAAA,CAAA,iBAAA,CAAA;AACA;AACA,OATA,EAUA,KAVA,CAUA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAZA,EAaA,OAbA,CAaA;AAAA,eAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AAAA,OAbA;AAcA,KA5BA;AA6BA,IAAA,QA7BA,sBA6BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,sBAAA,KAAA,GAAA;AACA,KA/BA;AAgCA,IAAA,MAhCA,oBAgCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA;AAlCA,GATA;AA6CA,EAAA,OA7CA,qBA6CA;AACA,SAAA,GAAA,GAAA,KAAA,EAAA;AACA,SAAA,gBAAA;AACA;AAhDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div v-if=\"!submitted\">\r\n      <form\r\n        action=\"\"\r\n        method=\"post\"\r\n        id=\"_frmForm\"\r\n        name=\"frmForm\"\r\n        @submit.prevent=\"addEmployee\"\r\n      >\r\n        <table class=\"content_table\">\r\n          <colgroup>\r\n            <col style=\"width: 30%;\" />\r\n            <col style=\"width: 70%;\" />\r\n          </colgroup>\r\n          <tr>\r\n            <th>제목</th>\r\n            <td>\r\n              <!-- 제목 -->\r\n              {{article.subject}}\r\n            </td>\r\n          </tr>\r\n          <tr>\r\n            <th>작성자</th>\r\n            <td>\r\n              <!-- 작성자 -->\r\n              {{article.writer}}\r\n            </td>\r\n          </tr>\r\n          <tr style=\"height: 100px\">\r\n            <th>내용</th>\r\n            <td>\r\n              <!-- 내용 -->\r\n              {{article.content}}\r\n            </td>\r\n          </tr>\r\n          <tr></tr>\r\n          <tr>\r\n            <th>작성일</th>\r\n            {{article.createDate}}\r\n            <td>\r\n              <!-- 작성일 -->\r\n            </td>\r\n          </tr>\r\n         \r\n        </table>\r\n      <button class=\"btn btn-success\" v-on:click=\"goList\">\r\n        돌아가기\r\n      </button>\r\n      <button class=\"btn btn-success\" v-on:click=\"deleted\">\r\n        삭제하기\r\n      </button>\r\n      <button class=\"btn btn-success\" v-on:click=\"modifyed\">\r\n        수정하기\r\n      </button>\r\n      </form>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../http-common4\";\r\nexport default {\r\n  name: 'QuestionItem',\r\n  data() {\r\n    return {\r\n      idx : 0,\r\n      article:[]\r\n    };\r\n  },\r\n  props: [\"id\"],\r\n  methods: {\r\n    selectApt: function(apt) {\r\n      this.$emit('select-apt', apt);\r\n    },\r\n    retrieveArticles() {\r\n      http\r\n        .get(\"/findArticleById/\"+this.idx)\r\n        .then(response => (this.article = response.data))\r\n        .catch(() => {\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    },\r\n    deleted(){\r\n      http\r\n        .post(\"/deleteArticle/\" + this.idx)\r\n        .then(response => {\r\n          if (response.data.state == \"succ\") {\r\n            alert(\"글을 삭제하였습니다.\");\r\n            this.$router.push(\"/question\");\r\n          } else {\r\n            alert(\"글을 삭제하지 못하였습니다.\");\r\n          }\r\n        })\r\n        .catch(() => {\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    },\r\n    modifyed(){\r\n      this.$router.push(\"/question/modify/\"+this.idx)\r\n    },\r\n    goList(){\r\n        this.$router.push(\"/question\")\r\n    }\r\n  },\r\n  mounted() {\r\n    this.idx = this.id;\r\n    this.retrieveArticles();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.img-list {\r\n  width: 50px;\r\n}\r\n.mouse-over-bgcolor {\r\n  background-color: lightblue;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}