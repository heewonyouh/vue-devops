\n        })\r\n        .finally(() => (this.loading = false));\r\n    },\r\n    searchname(){\r\n        http\r\n        .get(\"/findArticleBySubject/\"+this.cname)\r\n        .then(response => (this.articles = response.data))\r\n        .catch(() => {\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    }\r\n  },\r\n  mounted() {\r\n    this.retrieveArticles();\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}