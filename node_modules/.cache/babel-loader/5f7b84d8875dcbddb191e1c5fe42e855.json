\n        <col width=\"20%\" />\r\n        <col width=\"20%\" />\r\n        <col width=\"20%\" />\r\n        <col width=\"10%\" />\r\n        <tr>\r\n          <th>사원 아이디</th>\r\n          <th>사원명</th>\r\n          <th>부서</th>\r\n          <th>직책</th>\r\n          <th>연봉</th>\r\n        </tr>\r\n        <tr v-for=\"emp in info\" class=\"nicecolor\" :key=\"emp.id\">\r\n          <td v-html=\"emp.id\"></td>\r\n          <td v-html=\"emp.name\"></td>\r\n          <td v-html=\"emp.dept_id\"></td>\r\n          <td v-html=\"emp.title\"></td>\r\n          <td>{{ emp.salary | salarydecimal }}</td>\r\n        </tr>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../http-common\";\r\n\r\nexport default {\r\n  name: \"chart-customer\",\r\n  data() {\r\n    return {\r\n      deptid2: 0,\r\n      depcounts: [],\r\n      loading: true,\r\n      errored: false,\r\n      info: []\r\n    };\r\n  },\r\n  methods: {\r\n    totalmember() {\r\n      var a = 0;\r\n      for (var i = 0; i < this.depcounts.length; i++) {\r\n        a += this.depcounts[i].count;\r\n      }\r\n      return a;\r\n    },\r\n    transform(val1, val2) {\r\n      return `translate(${val1}, ${val2 * 30})`;\r\n    },\r\n    depcountheight() {\r\n      return this.depcounts.length * 30;\r\n    },\r\n    showme() {\r\n      return this.deptid2 != 0;\r\n    },\r\n    showdept(deptid) {\r\n      this.deptid2 = deptid;\r\n      http\r\n        .get(\"/findDepartmentsBydeptid/\" + deptid)\r\n        .then(response => (this.info = response.data))\r\n        .catch(() => {\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    }\r\n  },\r\n  mounted() {\r\n    http\r\n      .get(\"/findAllDepCounts\")\r\n      .then(response => (this.depcounts = response.data))\r\n      .catch(() => {\r\n        this.errored = true;\r\n      })\r\n      .finally(() => (this.loading = false));\r\n  },\r\n  filters: {\r\n    salarydecimal(value) {\r\n      var a = parseInt(value);\r\n      return a.toFixed(2);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.searchform {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n.search-result {\r\n  margin-top: 20px;\r\n  text-align: left;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}