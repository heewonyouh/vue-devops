 />\r\n    </div>\r\n    <table class=\"list_table\">\r\n      <col width=\"10%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"10%\" />\r\n      <tr>\r\n        <th>사원 아이디</th>\r\n        <th>사원명</th>\r\n        <th>부서</th>\r\n        <th>직책</th>\r\n        <th>연봉</th>\r\n      </tr>\r\n      <tr></tr>\r\n      <tr v-for=\"emp in cemps\" class=\"nicecolor\" :key=\"emp.id\">\r\n        <td v-html=\"emp.id\"></td>\r\n        <td v-html=\"emp.name\"></td>\r\n        <td v-html=\"emp.dept_id\"></td>\r\n        <td v-html=\"emp.title\"></td>\r\n        <td>{{ emp.salary | salarydecimal }}</td>\r\n      </tr>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../http-common\";\r\n\r\nexport default {\r\n  name: \"search-customer\",\r\n  data() {\r\n    return {\r\n      // info: [],\r\n      loading: true,\r\n      errored: false,\r\n      cname: \"\",\r\n      cemps: []\r\n    };\r\n  },\r\n  filters: {\r\n    salarydecimal(value) {\r\n      var a = parseInt(value);\r\n      return a.toFixed(2);\r\n    }\r\n  },\r\n  methods: {\r\n    searchname(e) {\r\n      if (e.which == 13) {\r\n        //enter\r\n        http\r\n          .get(\"/findLikeEmployees/\" + this.cname)\r\n          .then(response => (this.cemps = response.data))\r\n          .catch(() => {\r\n            this.errored = true;\r\n          })\r\n          .finally(() => (this.loading = false));\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.searchform {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n.search-result {\r\n  margin-top: 20px;\r\n  text-align: left;\r\n}\r\n</style>"]}]}