<option v-for=\"emp in info\" v-bind:value=\"emp.id\" :key=\"emp.id\">{{ emp.name }}</option>\r\n      </select>\r\n    </div>\r\n    <table class=\"list_table\">\r\n      <col width=\"10%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"20%\" />\r\n      <col width=\"10%\" />\r\n      <tr>\r\n        <th>사원 아이디</th>\r\n        <th>사원명</th>\r\n        <th>부서</th>\r\n        <th>직책</th>\r\n        <th>연봉</th>\r\n      </tr>\r\n      <tr>\r\n        <td v-html=\"cemp.id\"></td>\r\n        <td v-html=\"cemp.name\"></td>\r\n        <td v-html=\"cemp.dept_name\"></td>\r\n        <td v-html=\"cemp.title\"></td>\r\n        <td>{{ cemp.salary | salarydecimal }}</td>\r\n      </tr>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport http from \"../http-common\";\r\n\r\nexport default {\r\n  name: \"search-customer-id\",\r\n  data() {\r\n    return {\r\n      info: [],\r\n      loading: true,\r\n      errored: false,\r\n      cid: \"\",\r\n      cemp: {}\r\n    };\r\n  },\r\n  filters: {\r\n    salarydecimal(value) {\r\n      var a = parseInt(value);\r\n      return a.toFixed(2);\r\n    }\r\n  },\r\n  mounted() {\r\n    http\r\n      .get(\"/findAllEmployees\")\r\n      .then(response => (this.info = response.data))\r\n      .catch(() => {\r\n        this.errored = true;\r\n      })\r\n      .finally(() => (this.loading = false));\r\n  },\r\n  methods: {\r\n    findbyid() {\r\n      http\r\n        .get(\"/findEmployeeById/\" + this.cid)\r\n        .then(response => (this.cemp = response.data))\r\n        .catch(() => {\r\n          this.errored = true;\r\n        })\r\n        .finally(() => (this.loading = false));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.searchform {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n.search-result {\r\n  margin-top: 20px;\r\n  text-align: left;\r\n}\r\n</style>"]}]}